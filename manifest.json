{
  "manifest_version": 3,
  "name": "AdOps X-Ray",
  "version": "1.0.0",
  "description": "Professional AdOps debugging tool. Highlights ad units and detects SSP calls.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}